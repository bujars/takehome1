.data

f: .word 0
g: .word 50
h: .word 40
i: .word 30
j: .word 20

.text
lw $s0, f
lw $s1, g
lw $s2, h
lw $s3, i
lw $s4, j

#t0 = g + h
add $t0, $s1, $s2

# t1 = i + j
add $t1, $s3, $s4

#f = t1 - t0
sub $s0, $t0, $t1

sw $s0, f

# We expect to compute f = (g + h)  - (i + j) 
# Thus f = (50 + 40) - (30 + 20) 
#  = 40 and this will be stored in f. 